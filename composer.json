    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpunit/php-invoker": "*",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpspec/phpspec": "^7.0",
        "friends-of-phpspec/phpspec-code-coverage": "^6.0",
        "php-etl/phpspec-extension": "^0.2@dev",
        "phpstan/phpstan": "^0.12",
        "infection/infection": "^0.20"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
