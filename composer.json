{
    "name": "php-etl/csv-flow",
    "description": "CSV implementation for the ETL pipeline",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Flow\\Csv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "unit\\Kiboko\\Component\\Flow\\Csv\\": "tests/unit/",
            "functional\\Kiboko\\Component\\Flow\\Csv\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "psr/log": "^1.1@dev",
        "php-etl/pipeline-contracts": "^0.4@dev",
        "php-etl/bucket": "^0.3@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "phpunit/php-invoker": "*",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpspec/phpspec": "^7.0",
        "friends-of-phpspec/phpspec-code-coverage": "^6.0",
        "infection/infection": "^0.20",
        "adlawson/vfs": "dev-develop",
        "php-etl/phpunit-extension": "*",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^0.15"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev",
            "dev-feature/qualityflow-improvments": "0.3.x-dev"
        }
    }
}
